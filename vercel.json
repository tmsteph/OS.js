{
    "builds": [
      {
        "src": "src/server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "src/server/index.js" }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  